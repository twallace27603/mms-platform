{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "labId": {
      "type": "string",
      "defaultValue": "lods123456"
    },
    "labPassword": {
      "type": "string",
      "defaultValue": "Azure!Pa55w0rd"
    },
    "labNumber": {
      "type": "string",
      "defaultValue": "1"
    },
    "_artifactsLocation": {
      "type": "string"
    },
    "_artifactsLocationSasToken": {
      "type": "securestring"
    },
    "deploymentPackageFolder": {
      "type": "string",
      "minLength": 1,
      "metadata": {
        "description": "WebDeploy package location. This path is relative to the _artifactsLocation parameter"
      }
    },
    "deploymentPackageFileName": {
      "type": "string",
      "minLength": 1,
      "metadata": {
        "description": "Name of the webdeploy package"
      }
    },
    "siteName": {
      "type": "string",
      "defaultValue": "waps8083886"
    }
  },
  "variables": {
    "storageSku": "Standard_LRS",
    "saName": "[concat('sa', parameters('labId'))]"
  },
  "resources": [
    {
      "name": "[variables('saName')]",
      "type": "Microsoft.Storage/storageAccounts",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-01-01",
      "sku": {
        "name": "[variables('storageSku')]"
      },
      "dependsOn": [ ],
      "tags": {
        "displayName": "Storage account"
      },
      "kind": "Storage"
    },
    {
      "name": "[concat(parameters('siteName'), '/', 'MSDeploy')]",
      "type": "Microsoft.Web/sites/extensions",
      "location": "eastus",
      "apiVersion": "2015-08-01",
      "dependsOn": [ ],
      "tags": {
        "displayName": "deployment"
      },
      "properties": {
        "packageUri": "[concat(parameters('_artifactsLocation'), '/', parameters('deploymentPackageFolder'), '/', parameters('deploymentPackageFileName'), parameters('_artifactsLocationSasToken'))]",
        "dbType": "None",
        "connectionString": "",
        "setParameters": {
          "IIS Web Application Name": "[parameters('siteName')]"
        }
      }
    }],
  "outputs": {}
}
